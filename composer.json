{
    "name": "jacques/omnipay-adyen",
    "description": "Adyen driver for the Omnipay payment processing library",
    "keywords": [
        "omnipay",
        "adyen",
        "gateway",
        "merchant",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/jacques/omnipay-adyen",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za",
            "homepage": "http://www.siberia.co.za/"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/jacques/omnipay-adyen/contributors"
        }
    ],
    "require": {
        "omnipay/common": "^3.0.1"
    },
    "require-dev": {
        "omnipay/tests": "^3.1",
        "overtrue/phplint": "^1",
        "php-vcr/php-vcr": "^1.4",
        "php-vcr/phpunit-testlistener-vcr": "^3.2",
        "phpmd/phpmd": "^2",
        "phpro/grumphp": "^0.14",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Adyen\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Adyen\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
